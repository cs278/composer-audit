{
    "name": "cs278/composer-audit",
    "description": "Audit your Composer dependencies for security vulnerabilities.",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Smith",
            "email": "chris@cs278.org"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">= 7.1",
        "composer-plugin-api": "^1.1 || ^2",
        "symfony/yaml": "^4.4 || ^5"
    },
    "require-dev": {
        "cs278/mktemp": "^1",
        "composer/composer": "^1 || ^2@RC",
        "composer/semver": "*",
        "symfony/filesystem": "^4.4 || ^5",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/process": "^4.4 || ^5"
    },
    "conflict": {
        "symfony/yaml": "~4.4.27 || ~5.2.12 || ~5.3.4"
    },
    "autoload": {
        "psr-4": {
            "Cs278\\ComposerAudit\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "class": "Cs278\\ComposerAudit\\Legacy\\ComposerPlugin"
    }
}
